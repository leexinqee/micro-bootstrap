{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "import", 
        {
          "libraryName": "antd",
          "libraryDirectory": "lib",
          "style": "css" // `style: true` 会加载 less 文件
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        { "legacy": true }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {"loose": true}
      ],
      "@babel/plugin-transform-runtime"
    ]
  }
   
  